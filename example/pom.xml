<project xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>io.metaloom.test.example</groupId>
  <artifactId>postgresql-testdatabase-provider-example</artifactId>
  <version>0.0.1-SNAPSHOT</version>


  <properties>
    <postgres.driver.version>42.2.2</postgres.driver.version>
    <testcontainer.version>1.17.6</testcontainer.version>
  </properties>

  <dependencies>
    <dependency>
      <groupId>org.postgresql</groupId>
      <artifactId>postgresql</artifactId>
      <version>${postgres.driver.version}</version>
    </dependency>

    <!-- Test -->
    <dependency>
      <artifactId>postgresql-testdatabase-provider-client</artifactId>
      <groupId>io.metaloom.test</groupId>
      <version>0.0.1-SNAPSHOT</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.junit.jupiter</groupId>
      <artifactId>junit-jupiter-api</artifactId>
      <version>5.9.2</version>
      <scope>test</scope>
    </dependency>
  </dependencies>

</project>
  